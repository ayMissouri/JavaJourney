
public class App {
	public static void main(String [] args) {
		
		String name = "Bob";
		int age = 35;
		
		System.out.println("There once was a man named " + name + ",");
		System.out.println("he was 35 years old. He really liked");
		System.out.println("the name " + name + ", but he didn't like being 35.");
		System.out.println("Most of his friends were also 35, and one");
		System.out.println("of them was also named Bob.");
	}
}
